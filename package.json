{
  "name": "dota-hero-calculator-table",
  "version": "1.0.0",
  "private": true,
  "description": "Dota 2 Hero Calculator Table",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devilesk/dota-hero-calculator-table.git"
  },
  "author": "devilesk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devilesk/dota-hero-calculator-table/issues"
  },
  "scripts": {
    "build": "browserify -d -t brfs -x 'knockout' -x 'jquery' www/js/table.js > www/js/table.bundle.js"
  },
  "homepage": "https://github.com/devilesk/dota-hero-calculator-table#readme",
  "devDependencies": {
    "brfs": "^1.4.3",
    "browserify": "^13.1.0",
    "browserify-shim": "^3.8.12",
    "jquery": "^3.1.0",
    "jquery-ui": "^1.12.0",
    "knockout": "^3.4.0",
    "knockout-mapping": "^2.6.0",
    "minifyify": "^7.3.3"
  },
  "dependencies": {
    "dota-hero-calculator-library": "^1.0.4"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "knockout": "global:ko",
    "jquery": "global:$",
    "jquery-ui": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  }
}
